generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model user {
    id        String  @id @default(cuid())
    firstName String
    lastName  String?
    email     String  @unique
    imageUrl  String?
}

model politician {
    id String @id @default(cuid())
}
