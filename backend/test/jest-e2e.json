{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@app/prisma": "<rootDir>/../libs/prisma/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/guards/(.*)": "<rootDir>/../libs/guards/src/$1",
    "@app/guards": "<rootDir>/../libs/guards/src",
    "@app/decorators/(.*)": "<rootDir>/../libs/decorators/src/$1",
    "@app/decorators": "<rootDir>/../libs/decorators/src",
    "@app/openai_event_handler/(.*)": "<rootDir>/../libs/openai_event_handler/src/$1",
    "@app/openai_event_handler": "<rootDir>/../libs/openai_event_handler/src",
    "@app/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
    "@app/utils": "<rootDir>/../libs/utils/src",
    "@app/constants/(.*)": "<rootDir>/../libs/constants/src/$1",
    "@app/constants": "<rootDir>/../libs/constants/src"
  }
}